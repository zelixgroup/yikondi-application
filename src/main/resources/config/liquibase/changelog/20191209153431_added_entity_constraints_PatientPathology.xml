<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity PatientPathology.
    -->
    <changeSet id="20191209153431-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="patient_id"
                                 baseTableName="patient_pathology"
                                 constraintName="fk_patient_pathology_patient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="patient"/>

        <addForeignKeyConstraint baseColumnNames="pathology_id"
                                 baseTableName="patient_pathology"
                                 constraintName="fk_patient_pathology_pathology_id"
                                 referencedColumnNames="id"
                                 referencedTableName="pathology"/>

    </changeSet>
</databaseChangeLog>
