        <addForeignKeyConstraint baseColumnNames="dosage_form_id"
                                 baseTableName="drug"
                                 constraintName="fk_drug_dosage_form_id"
                                 referencedColumnNames="id"
                                 referencedTableName="drug_dosage_form"/>

    </changeSet>
</databaseChangeLog>
