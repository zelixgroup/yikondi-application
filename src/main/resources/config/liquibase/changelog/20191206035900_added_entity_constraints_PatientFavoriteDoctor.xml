<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity PatientFavoriteDoctor.
    -->
    <changeSet id="20191206035900-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="patient_id"
                                 baseTableName="patient_favorite_doctor"
                                 constraintName="fk_patient_favorite_doctor_patient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="patient"/>

        <addForeignKeyConstraint baseColumnNames="doctor_id"
                                 baseTableName="patient_favorite_doctor"
                                 constraintName="fk_patient_favorite_doctor_doctor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="doctor"/>

    </changeSet>
</databaseChangeLog>
